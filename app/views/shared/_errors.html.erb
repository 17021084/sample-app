<div class="row">
  <div class="col-xs-8 col-xs-offset-2 my-2">
    <div class="card bg-warning">
      <div class="card-header">
      </div>
      <div class="card-body">
        <div id="error_explanation">
          <ul>
            <% obj.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
